    "@sinclair/typebox": "^0.34.40",
    "commander": "^14.0.0",
    "elysia": "^1.3.13",
    "graphql": "^16.11.0",
    "graphql-yoga": "^5.15.1",
    "level": "10.0.0",
    "better-sqlite3": "12.2.0",
    "zstd-codec": "0.1.5"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "bun-types": "^1.2.20",
